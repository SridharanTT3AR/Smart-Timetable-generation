<!DOCTYPE html>
<html lang="en">
<head>
  <title></title>
  <script src="js/adminedit.js"></script>
</head>
<body>
  <h1>Course Table</h1>
  <table class="table table-hover table-bordered table-dark table-striped">
    <thead>
      <tr>
          <th scope="col">No</th>
          <th scope="col">Course_ID</th>
          <th scope="col">Name</th>
          <th scope="col">Type</th>
          <th scope="col">L</th>
          <th scope="col">P</th>
          <th scope="col">T</th>
          <th scope="col">C</th>
          <th scope="col">Department</th>
          <!-- <th scope="col">Edit</th> -->
          <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
      <%
      if(Data.length!=0){
      var i=1;
      Data.forEach(function(data){
      %>
      <tr>
          <td><%=i; %></td>
          <td><%=data.ID %></td>
          <td><%=data.Name %></td>
          <td><%=data.Type %> <%=data.E_ID %></td>
          <td><%=data.L %></td>
          <td><%=data.P%></td>
          <td><%=data.T%></td>
          <td><%=data.C %></td>
          <td><%=data.dept_name %></td>
          <!-- <td><a href="/users/edit/<%=data.id%>">Edit</a></td> -->
          <!-- <td><a href="/delete/<%=data.ID%>,Course"><i class="fas fa-times-circle"></i></a></td> -->
          <td> <button class="delete btn btn-outline-light" id="/delete/<%=data.ID%>,Course,<%=data.Type%>">delete</button> </td>
      </tr>
      <%  i++; }) %>
      <% } else{ %>
          <tr>
              <td colspan="7">No Data Found</td>
          </tr>
      <% } %>
    </tbody>
  </table>

</body>
</html>
