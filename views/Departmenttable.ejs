<!DOCTYPE html>
<html lang="en">
<head>
  <title></title>
  <script src="js/adminedit.js"></script>
</head>
<body>
  <h1>Department Table</h1>
  <table class="table table-hover table-bordered table-dark table-striped">
    <thead>
      <tr>
          <th scope="col">ID</th>
          <th scope="col">Name</th>
          <th scope="col">Building</th>
          <th scope="col">Hod_Id</th>
          <th scope="col">Ext_No </th>
          <!-- <th scope="col">Edit</th> -->
          <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
      <%
      if(Data.length!=0){
      var i=1;
      Data.forEach(function(data){
      %>
      <tr>
          <td><%=i; %></td>
          <td><%=data.Name %></td>
          <td><%=data.Building  %></td>
          <td><%=data.Hod_Id  %></td>
          <td><%=data.Ext_No %></td>
          <!-- <td><a href="/users/edit/<%=data.id%>">Edit</a></td> -->
          <!-- <td><a href="/delete/<%=data.Name%>,Department"><i class="fas fa-times-circle"></i></a></td> -->
          <td> <button class="delete btn btn-outline-light" id="/delete/<%=data.Name%>,Department">delete</button> </td>
      </tr>
      <%  i++; }) %>
      <% } else{ %>
          <tr>
              <td colspan="7">No Data Found</td>
          </tr>
      <% } %>
    </tbody>
  </table>

</body>
</html>
